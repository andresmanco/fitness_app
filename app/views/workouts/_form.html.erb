<div class="card">
  <%= form_for workout do |f| %>
  <h3>Pick up to 2 muscles</h3>
  <% muscle_groups.each do |muscle_group| %>
  <%= f.label muscle_group %>
  <%= f.check_box :muscles, {multiple: true}, muscle_group, false %><br>
  <% end %><br>
  <%= f.label "Do you have equipment?" %>
  <%= check_box_tag(:equipment) %><br>
  <%= f.submit %>
  <% end %>
</div>
